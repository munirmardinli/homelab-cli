@munirmardinli-dev:registry=${{ env.REGISTRY_URL }}
//npm.pkg.github.com/:_authToken=${{ secrets.GITHUB_TOKEN }}
prefer-offline=true
legacy-peer-deps=true
strict-peer-deps=true
loglevel=warn
package-lock=true
strict-ssl=true
maxsockets=50
fetch-retries=3
fetch-retry-mintimeout=1000
fetch-retry-maxtimeout=60000
progress=true
timing=true
audit=false
save-exact=true
engine-strict=true
