{
  "name": "homelab-cli",
  "description": "A CLI for managing your homelab infrastructure",
  "private": true,
  "version": "0.0.1",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "enginesStrict": true,
  "homepage": "https://munirmardinli.github.io/homelab-cli",
  "bin": {
    "homelab-cli": "config/bin/homelab-cli"
  },
  "packageManager": "npm@11.4.2",
  "sponsor": {
    "url": "https://linktr.ee/munirmardinli",
    "type": "individual",
    "name": "Munir Mardinli"
  },
  "publisher": "Munir Mardinli",
  "keywords": [
    "cli",
    "homelab",
    "infrastructure",
    "management",
    "typescript",
    "nodejs"
  ],
  "files": ["lib"],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "bugs": {
    "url": "https://github.com/munirmardinli/homelab-cli/issues",
    "email": "Munir@Mardinli.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/munirmardinli/homelab-cli.git",
    "directory": "Munir Mardinli",
    "homepage": "https://github.com/munirmardinli/homelab-cli#readme"
  },
  "author": {
    "name": "Munir Mardinli",
    "email": "Munir@Mardinli.de",
    "url": "https://linktr.ee/munirmardinli"
  },
  "funding": {
    "type": "individual",
    "url": "https://linktr.ee/munirmardinli"
  },
  "readmeFilename": "README.md",
  "contributors": [
    {
      "name": "Munir Mardinli",
      "email": "Munir@Mardinli.de",
      "url": "https://linktr.ee/munirmardinli"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yml,md}": ["prettier --write", "eslint --fix"],
    "*.ts": ["tsc --noEmit"]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.15",
    "dependency-cruiser": "^16.10.2",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "js-yaml": "^4.1.0"
  }
}
