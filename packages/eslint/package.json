{
  "name": "@homelab-cli/eslint",
  "version": "0.0.1",
  "sideEffects": false,
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./typescript": "./typescript.js",
    "./javascript": "./javascript.js",
    "./yaml": "./yaml.js",
    "./json": "./json.js"
  },
  "files": ["index.js", "typescript.js", "javascript.js", "yaml.js", "json.js"],
  "scripts": {
    "prettier": "prettier --write .",
    "eslint": "eslint . --fix",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "npm run eslint:check",
    "build": "echo 'No build step needed for ESLint config package.'",
    "lint:fix": "npm run eslint:fix",
    "typecheck": "echo 'No type checking needed for ESLint config package.'"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.28.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-yml": "^1.17.0",
    "yaml-eslint-parser": "^1.3.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "globals": "^16.0.0"
  }
}
