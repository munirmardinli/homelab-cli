{
  "name": "@homelab-cli/cli",
  "version": "0.0.1",
  "sideEffects": false,
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "postinstall": "tsc",
    "prettier": "prettier --write .",
    "eslint": "eslint . --fix",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "npm run eslint:check",
    "lint:fix": "npm run eslint:fix",
    "typecheck": "tsc --noEmit"
  },
  "files": ["lib"],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3",
    "@homelab-cli/eslint": "file:packages/eslint"
  },
  "dependencies": {
    "dotenv": "^17.0.1"
  }
}
