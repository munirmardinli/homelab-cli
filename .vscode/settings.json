{
	"editor.fontFamily": "'FiraCode-Retina','FiraCode Nerd Font Mono','Fira Code', 'Menlo','3270 Nerd Font','Hack Nerd Font', 'FiraCode', 'Menlo', 'Monaco', 'Courier New', monospace, '3270 Nerd Font',Cascadia Mono, Consolas, DejaVu Sans Mono",
  "terminal.integrated.fontFamily": "'FiraCode-Retina','FiraCode Nerd Font Mono','Fira Code', 'Menlo','3270 Nerd Font','Hack Nerd Font', 'FiraCode', 'Menlo', 'Monaco', 'Courier New', monospace, '3270 Nerd Font',Cascadia Mono, Consolas, DejaVu Sans Mono",
	"git.postCommitCommand": "sync",
	"workbench.colorTheme": "Default Dark Modern",
	"redhat.telemetry.enabled": true,
	"terminal.integrated.fontSize": 20,
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"window.zoomLevel": 1,
	"prettier.singleQuote": true,
	"prettier.endOfLine": "auto",
	"prettier.useTabs": true,
	"workbench.tree.indent": 12,
	"editor.minimap.enabled": false,
	"editor.stickyScroll.enabled": false,
	"editor.guides.bracketPairs": "active",
	"editor.scrollbar.horizontal": "hidden",
	"editor.renderLineHighlight": "gutter",
	"editor.accessibilitySupport": "on",
	"editor.guides.bracketPairsHorizontal": "active",
	"editor.tabSize": 2,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.lineNumbers": "on",
	"editor.fontLigatures": true,
	"editor.renderControlCharacters": false,
	"editor.fontSize": 24,
	"editor.lineHeight": 1.6,
	"editor.renderWhitespace": "none",
	"editor.suggestSelection": "first",
	"editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
	"editor.codeLens": false,
	"editor.insertSpaces": false,
	"editor.smoothScrolling": true,
	"editor.cursorBlinking": "expand",
	"editor.cursorStyle": "line-thin",
	"editor.cursorWidth": 3,
	"editor.mouseWheelZoom": true,
	"editor.mouseWheelScrollSensitivity": 1.5,
	"editor.fastScrollSensitivity": 2.5,
	"editor.scrollBeyondLastLine": true,
	"editor.codeActionsOnSave": {},
	"zenMode.fullScreen": true,
	"zenMode.hideLineNumbers": false,
	"javascript.suggest.autoImports": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"files.autoSave": "afterDelay",
	"javascript.preferences.quoteStyle": "single",
	"javascript.preferences.useAliasesForRenames": false,
	"typescript.preferences.useAliasesForRenames": false,
	"typescript.preferences.importModuleSpecifier": "relative",
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#599a43",
		"activityBar.activeBorder": "#599a43",
		"activityBar.background": "#391bfa",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#599a43",
		"activityBarBadge.foreground": "#e7e7e7",
		"statusBar.background": "#391bfa",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#391bfa",
		"titleBar.activeBackground": "#391bfa",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#391bfa",
		"titleBar.inactiveForeground": "#e7e7e799",
		"sash.hoverBorder": "#391bfa",
		"statusBarItem.remoteBackground": "#599a43",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799"
	},
	"[javascript][typescript][javascriptreact][typescriptreact]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.codeActionsOnSave": {
			"source.organizeImports.biome": "explicit"
		}
	},
	"[dockercompose]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"workbench.iconTheme": "vscode-icons",
	"git.autofetch": true,
	"explorer.confirmDelete": false,
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"explorer.confirmDragAndDrop": false,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[php]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true
	},
	"terminal.integrated.profiles.windows": {
		"Git Bash": {
			"source": "Git Bash",
			"path": "C:\\winserver\\Git\\bin\\bash.exe",
			"args": ["--login", "-i"]
		}
	},
	"breadcrumbs.enabled": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.tabs.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"terminal.integrated.defaultLocation": "editor",
	"zenMode.showTabs": "single",
	"[graphql]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "keyword.operator",
				"settings": {
					"foreground": "#A9A9A9",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"keyword.operator.equal",
					"keyword.operator.identity",
					"keyword.operator.arrow"
				],
				"settings": {
					"foreground": "#A9A9A9",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "constant.numeric",
				"settings": {
					"foreground": "#A9A9A9",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "constant.language",
				"settings": {
					"foreground": "#A9A9A9",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "entity.name.class",
				"settings": {
					"foreground": "#A9A9A9",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "entity.name.function",
				"settings": {
					"foreground": "#A9A9A9",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword",
				"settings": {
					"foreground": "#FF5722"
				}
			},
			{
				"scope": "variable",
				"settings": {
					"foreground": "#af7c4c"
				}
			},
			{
				"scope": "string",
				"settings": {
					"foreground": "#FFEB3B"
				}
			},
			{
				"scope": "comment",
				"settings": {
					"foreground": "#A9A9A9"
				}
			},
			{
				"scope": "invalid",
				"settings": {
					"foreground": "#FF0000",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "warning",
				"settings": {
					"foreground": "#FFA500",
					"fontStyle": "italic"
				}
			}
		]
	},
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"[dockerfile]": {
		"editor.defaultFormatter": "trunk.io"
	},
	"[ignore]": {
		"editor.defaultFormatter": "trunk.io"
	},
	"terminal.integrated.env.windows": {
		"LANG": "de_DE.UTF-8",
		"LC_ALL": "de_DE.UTF-8"
	},
	"files.encoding": "utf8",
	"eslint.workingDirectories": [
    { "directory": "packages/cli", "changeProcessCWD": true },
    { "directory": "packages/ssh", "changeProcessCWD": true }
  ],
	"tabnine.experimentalAutoImports": true,
	"files.exclude": {
		"**/.trunk/*actions/": true,
		"**/.trunk/*logs/": true,
		"**/.trunk/*notifications/": true,
		"**/.trunk/*out/": true,
		"**/.trunk/*plugins/": true
	},
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"cursor.diffs.useCharacterLevelDiffs": true,
	"cursor.terminal.usePreviewBox": true,
	"cursor.cpp.enablePartialAccepts": true,
	"editor.codeActions.triggerOnFocusChange": true,
	"git.commitMessageLanguage": "en",
	"git.inputValidationLength": 30,
	"codium.codeCompletion.enable": true,
	"yaml.customTags": [ 
	"!ENV scalar",
	"!ENV sequence",
	"!relative scalar",
	"tag:yaml.org,2002:python/name:material.extensions.emoji.to_svg",
	"tag:yaml.org,2002:python/name:material.extensions.emoji.twemoji",
	"tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format",
	"tag:yaml.org,2002:python/object/apply:pymdownx.slugs.slugify mapping"
]
}
